$(function(){
	var unitNumber=0;
	setTimeout(function(){
		unitNumber++;
		if(unitNumber>$(".slide_logo_box li").length-1){
			$(".slide_logo_box").stop().animate({"top":"0px"});
			unitNumber=0;
			}
		else{
			$(".slide_logo_box").stop().animate({"top":"-"+$(".slide_logo_box li:eq(0)").height()*unitNumber+"px"})
		}
		var _this=arguments.callee;
		setTimeout(_this,5000);
		},5000)
})


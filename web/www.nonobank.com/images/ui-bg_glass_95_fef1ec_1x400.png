<script type="text/javascript">
    $(function() {
        document.onkeydown = function(){};
        $('#loginpwd').keypress(function(e) {
        	e=e||event;
            if (e.keyCode == 13) {
                doLogin();
            }
        });
        $('#checkCode').keypress(function(e) {
        	e=e||event;
            if (e.keyCode == 13) {
                doLogin();
            }
        });
        function doLogin() {
            updateLoginWarningTips("登录中，请稍候...");
            var loginpwd = $('#loginpwd').val();
            $('#loginpwd').val(MD5(loginpwd));
            $.ajax({
                type: "POST",
                url: "/Login/AjaxLogin",
                data: $('#loginForm').serialize(),
                dataType: "json",
                success: function(data) {
                    if (data.result == 1) {
                        rememberme();
                        $("#loginDiv").dialog("close");
                        $.cookie("islogin", true, {expires:720});
                        islogin = true;                        
                        afterLogin(data.mid);
                                                $('#topmenu_reg').css('display', 'none');
                        $('#topmenu_login').css('display', 'none');
                        $('#topmenu_logout').css('display', '');
                        $('#toolbar_reg').css('display', 'none');
                        $('#toolbar_login').css('display', 'none');
                        $('#toolbar_logout').css('display', '');
                        $('#beforelogin').css('display', 'none');
                        $('#afterlogin').css('display', '');
                        $('#head_name').html(data.name);
                        $('#mymessage').html(data.msgcount);
                        $('#myinvest').html(data.earnings);
                        
                        // 登录成功之后，去异步登录bbs
                        $.ajax({
                			type: "POST",
                			url: "/Login/UcRsyncLogin/"+data.name+"/0/"+data.bbsToken,
                			success: function(data1) {
                				if(data1 != '') {
	                        		// bbs同步登录javascript语句
	                        		$("head").append(data1);
                        		}
                			}
                		});
                    }
                    else {
                        updateLoginWarningTips("");
                        updateLoginErroTips(data.msg);
                        validate_update('validate_img');
	                   	$('#validate_code_id').show();
		 				$("#checkcode_enable").val(true);
                        $('#loginpwd').val("");
                        $("#loginpwd").focus();
                        resizeLoginBox();
                    }

                }
            });
        }

        $("#loginDiv").dialog({
            width: 500,
            height: 300,
            autoOpen: false,
            modal: true,
            draggabled: true,
            title:"登录",
            buttons:
          {
              "登录": function() {
                  if (validlogin()) {
                      doLogin();
                  }
              },
              "不是会员": function() {
                  $(this).dialog("close");
                  clearLoginTips();
                  location.href = "/Register";
               }
          },
            close: function() {
                clearLoginTips();
            }
        });

    });
